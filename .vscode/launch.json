{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug and Watch",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/ts-node-dev",
      "outputCapture": "std",
      "args": ["${workspaceRoot}/src/index.ts"],
      "restart": true,
      "skipFiles": [
        "<node_internals>/**/*.js",
        "${workspaceFolder}/node_modules/**/*.js"
      ]
    }
  ]
}
