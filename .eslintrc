{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "parserOptions": {
    "ecmaVersion": 2017,
    "project": "./tsconfig.json"
  },
  "ecmaFeatures": {
    "jsx": false
  },
  "env": {
    "node": true
  },
  "extends": [
    "airbnb-typescript"
  ],
  "rules": {
    "max-len": "off",
    "no-console": "off"
  }
}
