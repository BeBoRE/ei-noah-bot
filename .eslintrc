{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "parserOptions": {
    "ecmaVersion": 2017,
    "project": "./tsconfig.json"
  },
  "env": {
    "node": true
  },
  "settings": {
    "react": {
      "version": "none"
    }
  },
  "extends": [
    "airbnb-typescript"
  ],
  "rules": {
    "max-len": "off",
    "no-console": "off"
  },
  "ignorePatterns": ["node_modules/"]
}
