{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["ESNext"],
    "target": "ESNext",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "baseUrl": "src",
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noUncheckedIndexedAccess": true
  },
  "exclude": ["node_modules/**"]
}
